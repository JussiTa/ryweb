<% if !['users'].include?(params[:controller]) || !['index'].include?(params[:action])%>
<% if current_user.user_level <= 10 %>
<li><%= link_to 'Käyttäjät', :controller => 'users', :action => 'list' %></li>				
	<% if current_user.user_level <= 5 %>
